  "scripts": {
    "precommit": "lint-staged",
    "start:js": "react-scripts-ts start",
    "start": "npm-run-all -p css:watch start:js",
    "start:no-browser": "BROWSER=none npm start",
    "css:build": "lessig src/",
    "css:watch": "lessig src/ --watch",
    "prebuild": "yarn run css:build",
    "build": "PUBLIC_URL='/' react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:watch": "yarn test -- --watchAll",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --config tslint.json --project tsconfig.json --type-check \"src/*.{ts,tsx} \" \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier:base": "prettier --parser typescript --trailing-comma none --single-quote \"src/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\"",
    "prettier:list": "npm run prettier:base -- --list-different",
    "prettier:write": "npm run prettier:base -- --write",
    "e2e": "cucumber-js features/*.feature"
  }
}
