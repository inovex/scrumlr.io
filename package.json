  "devDependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/classnames": "^2.2.0",
    "@types/enzyme": "^2.8.4",
    "@types/jest": "^19.2.3",
    "@types/lodash": "^4.14.74",
    "@types/node": "^7.0.21",
    "@types/react": "^15.6.0",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-copy-to-clipboard": "^4.2.3",
    "@types/react-dnd": "^2.0.32",
    "@types/react-dnd-html5-backend": "^2.1.7",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^4.0.14",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-swipe": "^5.0.1",
    "@types/react-tooltip": "^3.3.1",
    "@types/redux-logger": "^3.0.0",
    "enzyme": "^2.9.1",
    "husky": "^0.14.3",
    "less": "^2.7.2",
    "lessig": "^0.3.0",
    "lint-staged": "^4.0.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "react-scripts-ts": "^2.5.0",
    "react-test-renderer": "^15.6.1",
    "svg-inline-loader": "^0.8.0",
    "ts-keycode-enum": "^1.0.4"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start:js": "react-scripts-ts start",
    "start": "npm-run-all -p css:watch start:js",
    "start:no-browser": "BROWSER=none npm start",
    "css:build": "lessig src/",
    "css:watch": "lessig src/ --watch",
    "build": "yarn run css:build && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:watch": "yarn test -- --watchAll",
    "eject": "react-scripts-ts eject",
    "lint": "./node_modules/tslint/bin/tslint --config tslint.json --project tsconfig.json --type-check \"src/*.{ts,tsx} \" \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier:base": "prettier --parser typescript --trailing-comma none --single-quote \"src/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\"",
    "prettier:list": "npm run prettier:base -- --list-different",
    "prettier:write": "npm run prettier:base -- --write"
  }
}
