    "firebase": "^3.9.0",
    "lodash": "^4.17.4",
    "raven-js": "^3.17.0",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dd-menu": "^2.0.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.6.1",
    "react-dotdotdot": "^1.1.0",
    "react-redux": "^5.0.4",
    "react-redux-firebase": "^1.5.0-beta.3",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-swipe": "^5.1.0",
    "react-toastify": "^2.1.2",
    "react-tooltip": "^3.3.1",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "swipe-js-iso": "^2.0.4"
